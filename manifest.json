{
    "name": "Salesforce.com Interface Enhancer",
    "version": "0.0.0.8",
    "manifest_version": 2,
    "content_scripts": 
	[
		{
			"js": ["resources/js/jquery.js","resources/js/login.js"],
			"css": [ ],
			"matches": [ "https://login.salesforce.com/*" ],
			"run_at": "document_end"
		},{
			"js": ["resources/js/jquery.js","resources/js/test.js"],
			"css": [ ],
			"matches": [ "https://test.salesforce.com/*" ],
			"run_at": "document_end"
		},{
			"js": ["resources/js/jquery.js","resources/js/schemaBuilder.js" ],
			"css": [ ],
			"matches": [ "https://*.salesforce.com/_ui/platform/schema/ui/schemabuilder/*" ],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
    	"resources/js/jquery.js"
	],
    "background":{
        "scripts" : ["resources/js/background.js"]
    },
    "options_page": "options.html",	
    "permissions": 
	[
		"clipboardWrite",
		"tabs",  
		"https://*.salesforce.com/*"
	]	
}
